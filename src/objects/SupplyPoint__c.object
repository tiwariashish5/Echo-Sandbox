    <validationRules>
        <fullName>MeasuredSupplyPoint</fullName>
        <active>true</active>
        <description>If Measured = True, then Read Frequency must not be Unmeasured</description>
        <errorConditionFormula>AND ( ISPICKVAL(Measured__c, &apos;yes&apos;), ISPICKVAL( ReadFrequency__c , &apos;Unmeasured&apos;) )</errorConditionFormula>
        <errorMessage>Read Frequency must not be Unmeasured If Measured is yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SupplyPointCannotbeUnmeasured</fullName>
        <active>true</active>
        <description>If the user needs to add an active Supply Point Device to an Unmeasured Supply Point, he/she needs to update the linked Supply Point to Measured first.</description>
        <errorConditionFormula>ISPICKVAL(Measured__c ,&quot;No&quot;) &amp;&amp;  (TotalActiveSupplyPointDevice__c  &lt;&gt; 0)</errorConditionFormula>
        <errorMessage>Please update the related Supply Point to &quot;Measured&quot; first before adding a new active Supply Point Device.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UnmeasuredSupplyPoint</fullName>
        <active>true</active>
        <description>If Measured = False, then Read Frequency must be Unmeasured.</description>
        <errorConditionFormula>AND ( ISPICKVAL(Measured__c, &apos;No&apos;),  NOT(ISPICKVAL( ReadFrequency__c , &apos;Unmeasured&apos;)) )</errorConditionFormula>
        <errorMessage>Read Frequency must be Unmeasured if measured is selected as No.</errorMessage>
    </validationRules>
